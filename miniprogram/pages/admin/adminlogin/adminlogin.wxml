<van-cell-group>
  <van-field model:value="{{ username }}" required clearable label="管理员账号" icon="question-o" placeholder="请输入账号" />

  <van-field model:value="{{ password }}" type="password" label="密码" placeholder="请输入密码" required border="{{ false }}" />

  <van-button color="#ccc" plain round block bindtap="login">登录</van-button>
</van-cell-group>
